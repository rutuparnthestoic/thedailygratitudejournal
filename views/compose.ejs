<%- include('partials/header'); %>
<h1>Write an Entry</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="newTitle">Title</label>
        <input type="text" name="newTitle" class="form-control">
        <label for="newContent">Post</label>
        <textarea name="newContent" class="form-control"></textarea>
    </div>
    <button type="submit" name="" value="" class="btn btn-info">Publish</button>
</form>

